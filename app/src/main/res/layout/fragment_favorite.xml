<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FavoriteFragment">

    <TextView
        android:id="@+id/favorite_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Favoriter"
        android:gravity="center"
        android:padding="20dp"
        android:textSize="48sp"
        android:layout_margin="10dp"
        />
    <Button
        android:id="@+id/sql_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Lagra sql"/>
    <Button
        android:id="@+id/sql_read_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LÃ¤s SQL"/>
    <TextView
        android:id="@+id/sqlTable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/fragmentContainer"
        android:layout_centerHorizontal="true" />
    <ListView
        android:id="@+id/favorite_listView"
        android:layout_below="@+id/favorite_title"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="@string/hello_blank_fragment" />

</RelativeLayout>